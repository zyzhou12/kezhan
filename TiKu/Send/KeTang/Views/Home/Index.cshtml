                            <i class="u-icon iconfont icon-dian" 
                              v-on:click="setThin(50)"></i><i class="u-icon iconfont icon-dian" 
                                v-on:click="setThin(100)"></i><i class="u-icon iconfont icon-dian" 
                                  v-on:click="setThin(150)"></i>
                          </div>
                        </div>
                        <i class="u-icon iconfont icon-zititubiaoxiugai-"></i>
                      </div>                      
                      <div class="u-tooltip" onclick="showDetail(this)">
                        <div class="m-kit m-kit-color" style="display: none">
                          <div class="u-grid-col flex-6"><i class="u-icon iconfont icon-fangkuang1" 
                                v-on:click="setType(BoardSDK.DRAW_TYPE.SELECT)"></i><i class="u-icon iconfont icon-dian" 
                                  v-on:click="setType(BoardSDK.DRAW_TYPE.POINTSELECT)"></i>
                          </div>
                        </div>
                        <i class="u-icon iconfont icon-fangkuang1"></i>
                      </div>
                      <div class="u-tooltip" v-on:click="setType(BoardSDK.DRAW_TYPE.ERASER)">
                        <i class="u-icon iconfont icon-xiangpica"></i>
                        <div class="u-tooltip-title">
                          橡皮擦</div>
                      </div>
                      <div class="u-tooltip" onclick="showDetail(this)">
                        <div class="m-kit m-kit-color" style="display: none">
                          <div class="u-grid-col flex-6">
                            <i class="u-icon iconfont icon-chexiao2" 
                              v-on:click="revert"></i>
                            <i class="u-icon iconfont icon-huifu" 
                              v-on:click="process"></i>
                            <i class="u-icon iconfont icon-qingping" 
                              v-on:click="clearDraws"></i><i class="u-icon iconfont icon-qingping1" 
                                v-on:click="clear"></i>
                          </div>
                        </div>
                        <i class="u-icon iconfont icon-qingping1"></i>
                      </div>

                                          
                      <div class="u-tooltip" onclick="showDetail(this)">
                        <div class="m-kit m-kit-color" style="display: none">
                          <div class="u-grid-col flex-6"><i class="u-icon iconfont icon-baiban" onclick="document.getElementById('divPage').style.display='block';"
                                v-on:click="addBoard"></i><i class="u-icon iconfont icon-huishoushanchu" 
                                  v-on:click="deleteBoard"></i><i class="u-icon iconfont icon-huishoushanchu" 
                                  v-on:click="deleteBoard2"></i>
                          </div>
                        </div>
                        <i class="u-icon iconfont icon-baiban"></i>
                      </div>
                    </div>
                    <div class="m-kit-wb-pagination" style="display: none" id="divPage">
                      <div class="u-tooltip">
                        <div>
                          <div class="u-pagination">
                            <a class="u-icon iconfont icon-back disable" v-on:click="prevBoard"></a><span class="u-pagination-text">
                              {{boardData.page.current}} / {{boardData.page.total}}</span><a class="u-icon iconfont icon-more "
                                v-on:click="nextBoard"></a>
                          </div>
                        </div>
                        <div class="u-tooltip-title">
                          分页</div>
                      </div>
                    </div>
                    <div class="m-kit-wb-file">
                      <div class="u-tooltip m-kit-wb-file-item" id="divOpenFile">
                        <input id="file_input" style="position: absolute; opacity: 0;" type="file" v-on:change="uploadFile"
                          accept="image/*, application/pdf, application/msword, application/vnd.ms-powerpoint, application/vnd.ms-excel" />
                        <i class="u-icon iconfont u-file-icon">文档库</i><div class="u-tooltip-title">
                          打开文档库</div>
                      </div>
                      <div class="u-tooltip m-kit-wb-file-item" style="display: none" id="divCloseFile">
                        <i class="u-icon iconfont u-file-icon">关闭文档</i><div class="u-tooltip-title">
