                            </i><i class="u-icon iconfont icon-quxian" v-on:click="setType(BoardSDK.DRAW_TYPE.LINE)">
                            </i>
                            <i class="u-icon iconfont icon-tuoyuan" v-on:click="setType(BoardSDK.DRAW_TYPE.GRAPH_OVAL)">
                            </i><i class="u-icon iconfont icon-fangkuang1" v-on:click="setType(BoardSDK.DRAW_TYPE.GRAPH_RECT)">
                            </i>
