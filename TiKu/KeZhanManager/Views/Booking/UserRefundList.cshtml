@model KeZhanManager.Models.UserRefundListModel
<table class="table table-border table-bordered table-hover table-bg table-sort">
    <thead>
        <tr class="text-c">
            <th width="25"><input type="checkbox" name="" value=""></th>
            <th width="120">退款单号</th>
            <th width="130">订单号</th>
            <th width="50">申请金额</th>
            <th>申请备注</th>
            <th width="50">退款金额</th>
            <th>退款备注</th>
            <th width="120">退款时间</th>
            <th width="70">状态</th>
            <th width="100">操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var refund in Model.refundList)
        {
            <tr class="text-c">
                <td><input type="checkbox" value="@refund.fID" name=""></td>
                <td>@refund.fOrderNo</td>

                <td>@refund.fBookingNo</td>
                <td>
                    @refund.fApplyAmount
                </td>
                <td>@refund.fApplyRemark</td>
                <td class="text-l">@refund.fRefundAmount</td>
                <td>@refund.fRefundNote</td>
                <td>@refund.fRefundDate</td>
                <td class="td-status">
                    @if (refund.fStatus == 0)
                    {<span class="label label-success radius">提交</span>}
                    else if (refund.fStatus == 1)
                    {<span class="label label-success radius">已退款</span>}
                    else if (refund.fStatus == 2)
                    {<span class="label label-success radius">已驳回</span>}
            </td>
            <td class="td-manage">
                <a style="text-decoration:none" onclick="article_shenhe(this,'10001')" href="javascript:;" title="审核">审核</a>
            </td>
        </tr>
        }
    </tbody>
</table>