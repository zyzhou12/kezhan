<body>
  <div id="app_box">
    <div class="m-login">
      <div class="form-part">
        <div class="form-part-inner">
          <div class="title">
            在线教育</div>
          <div class="subtitle">
            Web Demo</div>
          <div class="box">
            <div class="form-item">
              <select id="account" name="account" class="form-control col-sm-10">
                <option value="Web_trtc_01">Web_trtc_01</option>
                <option value="Web_trtc_02">Web_trtc_02</option>
                <option value="Web_trtc_03">Web_trtc_03</option>
                <option value="Web_trtc_04">Web_trtc_04</option>
              </select></div>
            <div class="form-item">
              <label class="form-label">
                请输入密码</label><input type="password" value="" class="u-input"></div>
            <div class="form-item f-tar">
              <div hidden="hidden" class="errortip">
              </div>
              <div>
                <button class="u-btn">
                  登录</button></div>
              <div>
                <button class="u-btn u-btn-none">
                  注册</button></div>
            </div>
          </div>
        </div>
      </div>
      <div class="banner">
      </div>
    </div>
    <div style="display: none;">
      <div class="m-header">
        <div class="logo">
          在线教育DEMO</div>
        <div class="opt">
          <div class="opt-inner">
            <a href="javascript:void(0);" class="logout">用户</a></div>
        </div>
      </div>
      <div class="m-home">
        <div class="part part-1">
          <div class="icon icon-createroom">
          </div>
          <div class="item item-1 f-tac">
            <input placeholder="请输入房间名称" value="" class="u-input"></div>
          <div hidden="hidden" class="errortip ">
          </div>
          <div class="item item-2  f-tac">
            <button disabled="disabled" class="u-btn u-btn-longer">
              创建房间</button></div>
        </div>
        <div class="part part-2">
          <div class="icon icon-joinroom">
          </div>
          <div class="item item-1 f-tac">
            <input name="roomId" placeholder="请输入房间ID号码" value="" class="u-input"></div>
          <div hidden="hidden" class="errortip ">
          </div>
          <div class="item item-2  f-tac">
            <button disabled="disabled" class="u-btn u-btn-longer">
              加入房间</button></div>
          <div class="tip">
            注：房间ID为创建房间后 生成的8位数字</div>
        </div>
      </div>
    </div>
  </div>
  <!-- WebRTC SDK -->
  <script src="https://sqimg.qq.com/expert_qq/webrtc/2.6.2/WebRTCAPI.min.js"></script>
  <!-- WebIM SDK -->
  <script src="https://sqimg.qq.com/expert_qq/webim/1.7.1/webim.min.js"></script>
  <!-- 白板SDK -->
  <script src="https://sqimg.qq.com/expert_qq/edu/2.2.3/board_sdk.mini.js"></script>
  <!-- <script src="./libs/board_sdk.mini.js"></script> -->
  <!-- <script src="./libs/sketchpad_sdk.mini.js"></script> -->
  <!-- COS SDK -->
  <script src="https://sqimg.qq.com/expert_qq/cos/5.0.0/cos.mini.js"></script>
  <!-- TIC SDK -->
  <!-- <script src="./libs/TICSDK.js"></script> -->
  <script src="https://sqimg.qq.com/expert_qq/TICSDK/1.3.1/TICSDK.mini.js"></script>
  <script src="../../Content/js/account.js"></script>
  <script src="../../Content/js/app.js"></script>
</body>
